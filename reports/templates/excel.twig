{% xlsdocument %}
    {% xlssheet 'Hellow' {
			'columnDimension': {
				'A': {
					'width': '5'
				},
				'B': {
					'width': '25'
				}
			}
	} %}
        {% xlsrow %}
            {% xlscell { style: { font: { size: '18' } } } %}Values{% endxlscell %}
        {% endxlsrow %}
        {% for value in data %}
            {% xlsrow %}
                {% xlscell {
                'style': {
                    'font': {
                        'size': '18'
                    },
				
                },
                'url': 'http://example.com/'
            }
				
				
				 %}{{ value }}
				{% endxlscell %}
            {% endxlsrow %}
        {% endfor %}
    {% endxlssheet %}
{% endxlsdocument %}
